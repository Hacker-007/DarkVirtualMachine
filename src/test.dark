@main
    -- Define a variable called a
    set a 1

    -- Increment a by 1
    push 1
    push a
    push add

    -- Print out the value of incrementing a by 1
    printn peek

    -- Print the message 'The Value Of a Is: 1'
    push a
    push 'The Value Of a Is: '
    push add
    printn pop

    -- Set the value of a to be a + 1
    set a pop

    -- Print the message 'The Value Of a Is: 1'
    push a
    push 'The Value Of a Is: '
    push add
    printn pop
end